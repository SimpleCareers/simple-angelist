       <fa-modifier
          id="swipeupMod"
          fa-origin="[.5, 0]"
          fa-align="[0.5, 0]"
          fa-translate="[0,8.5]"
        >
        <fa-image-surface
          class="apply-layer"
          fa-image-url="/images/assets/swipeup.png"
          fa-size="[474/2,8.5]"
          >
        </fa-image-surface>
        </fa-modifier>
         
