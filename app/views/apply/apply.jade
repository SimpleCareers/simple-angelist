fa-container-surface(fa-options="options.containerSurface")
  fa-scroll-view(fa-start-index="0", fa-pipe-from="listPipe", fa-options="options.listScrollView")
    fa-surface(class="apply-content", fa-pipe-to="listPipe", fa-size="[undefined,true]")
      div(ng-repeat="like in likes")
        div.apply-media
          img.company-img
          div.media-body
            h4.title
              | {{like.title}}
            p.subtitle
              | {{card.startup.name}} 
              span(ng-if="card.location")
                | • {{card.location}}
            p.description
              | {{card.job_type.capitalize()}} • 
              | ${{(card.salary_min/1000|| 0) | number}}K - ${{(card.salary_max/1000|| 0) | number}}K • 
              | {{(card.equity_min || 0) | number:1}}% - {{(card.equity_max || 0) | number:1}}%
        .hline
        .margin-bottom-100(ng-if="$index==rows.length-1")
