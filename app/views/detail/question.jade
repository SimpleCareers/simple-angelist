fa-view
  fa-modifier(fa-translate="getApplyPosition()")
    fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]")
      fa-surface.apply-menu.apply-layer(fa-pipe-to="applyPipe", fa-background-color="'#DD4F3C'", fa-size="[320,64]")
    fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]", fa-translate="[0, 30]")
      fa-surface.apply-menu.apply-layer(fa-pipe-to="applyPipe", fa-background-color="'#DD4F3C'", fa-size="[320,64]")
        fa-modifier(fa-origin="[0,0]", fa-align="[0,0]", fa-translate="[0,45/2]")
          fa-sequential-layout(fa-options="options.applyMenuList")
            fa-view(ng-repeat="menu in applymenus")
              fa-surface.apply-menu-item(fa-size="[320/3,true]")
                | {{menu}}
                h1 Hello World
    fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]", fa-translate="[0,64]", fa-size="[undefined,504]")
      fa-scroll-view(fa-start-index="0", fa-pipe-from="listPipe", fa-options="options.listScrollView")
        fa-view(fa-size="[320.5,175/2]", ng-repeat="menu in rows track by $index")
          fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]")
            fa-surface(fa-background-color="white", fa-size="[undefined,175/2]", fa-pipe-to="listPipe")
          fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]", fa-translate="[25/2,25/2]")
            fa-image-surface.list-item-thumb(fa-image-url="/images/assets/logosq.png", fa-size="[55,55]", fa-pipe-to="listPipe")
          fa-surface.apply-layer(fa-pipe-to="listPipe")
            fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]", fa-translate="[78,16]")
              fa-surface.list-h1.apply-layer(fa-size="[150,20.5]")
                | {{menu}}
            fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]", fa-translate="[78,36]")
              fa-surface.list-h2.apply-layer(fa-size="[170.5,15.5]")
                | {{menu}}
            fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]", fa-translate="[78,53.5]")
              fa-surface.list-h2.apply-layer(fa-size="[220.5,15.5]")
                | {{menu}}
          fa-modifier(fa-origin="[0, 0]", fa-align="[0, 0]")
            fa-image-surface(fa-image-url="/images/assets/hline.png", fa-pipe-to="listPipe", fa-size="[undefined,true]")

